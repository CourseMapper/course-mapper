<div class="box-body no-padding" ng-controller="CourseListCreatedController">
    <table class="table table-striped">
        <tbody>
        <tr>
            <th style="width:10px;">Nr.</th>
            <th>{{headerTitle}}</th>
        </tr>
        <tr ng-repeat="created in courseCreated">
            <td>{{$index+1}}.</td>
            <td><a href="/course/{{created.slug}}/#/cid/{{created._id}}"> {{created.name}} </a></td>
        </tr>
        </tbody>
    </table>
</div>

