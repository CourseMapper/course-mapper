<% include header %>
<div class="wrapper" ng-controller="adminController as adm">

    <!-- Header Navbar -->
    <% include mainMenu %>

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper content-wrapper-admin" >
        <section class="content-header">
            <ol class="breadcrumb">
                <li><a href="/cm-admin/"><i class="fa fa-home"></i> Admin Home</a></li>
                <li ng-repeat="bread in $route.current.locals.pd.breads">
                    <a ng-href="{{bread.a}}" ng-class="{'active':bread.active}">{{bread.title}}</a>
                </li>
            </ol>
            <h1>
                {{$route.current.locals.pd.title}}
            </h1>
        </section>
        <section class="content" ng-view>
        </section>
    </div><!-- /.content-wrapper -->

</div><!-- ./wrapper -->

<% include ../jsfooter %>
<script src="/angular/courseMapperAdmin.js"></script>
<script src="/datatables/jquery.dataTables.js" type="text/javascript"></script>
<script src="/datatables/dataTables.bootstrap.js" type="text/javascript"></script>
<% include ../footer %>