                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star-o"></i>
                                    </div>
                                    <a href="/catalogs/web-tech"><img src="/img/minitree.png"></a>
                                    <div class="form-group">
                                    The things that are related to Web Tech
                                </div>
                                    <div class="form-group">
                                        Contributors:
                                        <ul>
                                            <li>John Doe</li>
                                            <li>Jane Doe</li>
                                            <li>John Doe</li>
                                            <li>Jane Doe</li>
                                            <li>John Doe</li>
                                            <li>Jane Doe</li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane" id="dashboard">

                            </div>
                            <div class="tab-pane" id="discussion"></div>
                            <div class="tab-pane" id="updates"></div>
                            <div class="tab-pane" id="analytics"></div>
                        </div>
                    </div>
                    <!-- /tabs -->

