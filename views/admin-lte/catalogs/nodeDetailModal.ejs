<!-- node edit form -->
<div class="row">
    <div class="modal" id="editContentNodeModal" tabindex="-1" role="dialog"
         aria-labelledby="edit view" aria-hidden="true">
        <div ng-include="'/course/editContentNode'"></div>
    </div>
</div>

<!-- widgetGallery for course analytics -->
<div class="row" ng-controller="WidgetGalleryController">
    <div class="modal" id="widgetGalleryNodeAnalytics" tabindex="-1" role="dialog"
         aria-labelledby="wigets gallery node Analytics" aria-hidden="true" ng-init="initData('node-analytics')">
        <div ng-include="'/widgets/gallery'"></div>
    </div>
</div>
 
<!-- course setting form -->
<div class="row">
    <div class="modal" id="configView" tabindex="-1" role="dialog"
         aria-labelledby="config view" aria-hidden="true">
        <% include nodeConfigModal %>
    </div>
</div>